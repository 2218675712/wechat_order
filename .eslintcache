\\services\\http.js",[],"D:\\学习\\前端\\自学前端\\16-react\\微信授权扫码点餐-新特性React16\\源代码\\wechat-order\\src\\services\\getData\\welcome.js",["144"],"import http from \"../http\";\r\nimport {getShopData,getUserInfo,addPeople} from '../apis/welcome'\r\n\r\nexport default {\r\n    getShopData(id) {\r\n        return http.get(getShopData,{\r\n            params:{\r\n                id\r\n            }\r\n        })\r\n    },\r\n    getUserInfo(id) {\r\n        return http.get(getUserInfo,{\r\n            params:{\r\n                id\r\n            }\r\n        })\r\n    },\r\n    addPeople(arg) {\r\n        return http.post(addPeople,{...arg})\r\n    }\r\n}\r\n","D:\\学习\\前端\\自学前端\\16-react\\微信授权扫码点餐-新特性React16\\源代码\\wechat-order\\src\\services\\index.js",[],"D:\\学习\\前端\\自学前端\\16-react\\微信授权扫码点餐-新特性React16\\源代码\\wechat-order\\src\\utils\\tool.js",[],"D:\\学习\\前端\\自学前端\\16-react\\微信授权扫码点餐-新特性React16\\源代码\\wechat-order\\src\\services\\apis\\welcome.js",[],"D:\\学习\\前端\\自学前端\\16-react\\微信授权扫码点餐-新特性React16\\源代码\\wechat-order\\src\\pages\\Welcome\\SelectPeople\\index.jsx",["145","146"],"import React, {useEffect, useState} from 'react'\r\nimport {Route} from 'react-router-dom'\r\nimport Styles from './index.module.less'\r\nimport {getParams} from \"../../../utils/tool\";\r\nimport {welcome} from \"../../../services\";\r\n\r\nconst SelectPeople = (props) => {\r\n    const [activeNum, setactiveNum] = useState(0)\r\n    const [tableNum, settableNum] = useState(0)\r\n    useEffect(() => {\r\n        settableNum(getParams(\"tableNum\"))\r\n    }, [])\r\n    /*    const submit = message => {\r\n            let data = {\r\n                shopId: getParams(\"shopId\"),\r\n                userId: getParams(\"userId\"),\r\n                tableNum: getParams(\"tableNum\"),\r\n                people: activeNum\r\n            }\r\n            welcome.addPeople(data).then((data) => {\r\n                console.log(data)\r\n                alert('提交成功', 'success', 1000)\r\n            })\r\n        }*/\r\n    const submit = () => {\r\n        let data = {\r\n            shopId: getParams(\"shopId\"),\r\n            userId: getParams(\"userId\"),\r\n            tableNum: getParams(\"tableNum\"),\r\n            people: activeNum\r\n        }\r\n        welcome.addPeople(data).then((data) => {\r\n            alert('提交成功', 'success', 1000)\r\n            setTimeout(() => {\r\n                props.history.push(\"/project/list\" + '?shopId=' + getParams(\"shopId\") + '&userId=' + getParams(\"userId\") + '&tableNum=' + getParams(\"tableNum\"))\r\n            }, 1000)\r\n        })\r\n    }\r\n    return <div className={Styles.selectPeople}>\r\n        <h3>{tableNum}号</h3>\r\n        <div>用餐人数</div>\r\n        <span>请选择正确的用餐人数</span>\r\n        <ul>\r\n            {\r\n                [...new Array(8)].map((item, index) => {\r\n                    return <li\r\n                        key={index}\r\n                        className={activeNum == index + 1 ? Styles.active : \"\"}\r\n                        onClick={() => setactiveNum(index + 1)}>{index + 1}</li>\r\n                })\r\n            }\r\n        </ul>\r\n        <p className={Styles.btn} onClick={submit}>开始点菜</p>\r\n    </div>\r\n}\r\nconst RenderRouter = () => {\r\n    return <Route path='/welcome/selectPeople' exact component={SelectPeople}/>\r\n}\r\nexport default RenderRouter\r\n","D:\\学习\\前端\\自学前端\\16-react\\微信授权扫码点餐-新特性React16\\源代码\\wechat-order\\src\\pages\\Welcome\\index.jsx",[],"D:\\学习\\前端\\自学前端\\16-react\\微信授权扫码点餐-新特性React16\\源代码\\wechat-order\\src\\components\\Msg\\index.jsx",["147","148"],"import Styles from './index.module.less'\r\nimport {observer} from 'mobx-react'\r\nimport {dialog} from '../../store'\r\n\r\nconst Msg = observer((props) => {\r\n    let {msg, type, show} = props.dialog\r\n    return show ? <div className={Styles.msg}>\r\n        <main>\r\n            <i className={type==\"success\"?\"iconfont icon-prompt_fill \"+Styles.success:\"iconfont icon-prompt_fill \"+Styles.error}